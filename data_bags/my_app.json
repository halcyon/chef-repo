{
  "group": "vagrant",
  "repository": "git://github.com/edavis10/redmine.git",
  "server_roles": [
    "my_app"
  ],
  "migrate": {
    "production": "false"
  },
  "force": {
    "production": "false"
  },
  "snapshots_to_keep": {
    "production": "if using EBS, integer of the number of snapshots we're going to keep for this environment."
  },
  "packages": {
    "curl": ""
  },
  "databases": {
    "production": {
      "encoding": "utf8",
      "reconnect": "true",
      "adapter": "mysql",
      "username": "db_user",
      "database": "db_name_production",
      "host": "localhost",
      "password": "sun"
    }
  },
  "database_slave_role": [
    "my_app_database_slave"
  ],
  "memcached": {
    "production": {
      "namespace": "my_app_environment"
    }
  },
  "mysql_repl_password": {
    "production": "sun"
  },
  "id": "my_app",
  "database_master_role": [
    "my_app_database_master"
  ],
  "type": {
    "my_app": [
      "rails"
    ]
  },
  "revision": {
    "production": "1.1.3"
  },
  "gems": {
    "rails": "2.3.5",
    "mysql": "",
    "rack": "1.1.0",
    "i18n": "0.4.2"
  },
  "rake": {
    "before_migrate": [
      "generate_session_store"
    ],
    "after_migrate": [
      "redmine:load_default_data"
    ]
  },
  "memcached_role": [
    "my_app_memcached"
  ],
  "deploy_key": "SSH private key used to deploy from a private git repository",
  "mysql_root_password": {
    "production": "sun"
  },
  "owner": "vagrant",
  "deploy_to": "/srv/my_app",
  "mysql_debian_password": {
    "production": "sun"
  }
}
